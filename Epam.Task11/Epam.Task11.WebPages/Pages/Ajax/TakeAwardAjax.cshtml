@{
    if (IsAjax)
    {
        int.TryParse(Request.Params["user_id"], out var userId);
        int.TryParse(Request.Params["award_id"], out var awardId);

        if (userId > 0 && awardId > 0)
        {
            if (Epam.Task11.WebPages.CS.BLLProvider.AwardLogic.TakeAward(userId, awardId))
            {
                @(1)
            }
        }

        return;
    }
}